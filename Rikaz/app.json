{
  "expo": {
    "name": "Rikaz",
    "slug": "Rikaz",
    "owner": "rikazqp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/RikazLogo.png",
    "scheme": "rikaz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rikaz.app"
    },
    "android": {
      "package": "com.rikaz.app",
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/RikazLogo.png",
        "backgroundImage": "./assets/images/RikazLogo.png",
        "monochromeImage": "./assets/images/RikazLogo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/RikazLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/RikazLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a62e3c96-60d6-4067-baf8-bc72f893972f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a62e3c96-60d6-4067-baf8-bc72f893972f"
    }
  }
}
